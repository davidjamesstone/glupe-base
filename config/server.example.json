{
  "server": {
    "host": "127.0.0.1",
    "port": 3000
  },
  "db": {
    "uri": "mongodb://localhost/myapp"
  },
  "logging": {
    "ops": {
      "interval": 1000
    },
    "reporters": {
      "console": [
        {
          "module": "good-squeeze",
          "name": "Squeeze",
          "args": [
            {
              "log": "*",
              "error": "*",
              "response": "*"
            }
          ]
        },
        {
          "module": "good-console"
        },
        "stdout"
      ]
    }
  },
  "views": {
    "cache": false
  }
}
